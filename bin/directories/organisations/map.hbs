{
	"type":"FeatureCollection","name":"orgs",
	"features":[
	{{#each this}}
		{
			"type":"Feature",
			"properties":{
				"type": "Organization",
				"html": [
					"<div ><img class='gridElementLogo' src='{{img}}'/></div>",
					"<div class='label-location' style='display: flex;flex-direction: row;'><h3>{{city}}</h3>{{#if country}}<img alt='{{country}}' class='flag' src='{{flag}}' style='margin-left: auto;'/>{{/if}}</div>",
					"<div class='ihub-name'>{{name}}</div>",
					"<div class='ihub-type'>{{type}}</div>"{{#if website}},
					"<div class='icons'><a class='cat-website' target='_blank' href='{{website}}' rel='noopener'><span class='icon website'></span></div>"{{/if}}
				]
			},
			"geometry":{
				"type":"Point",
				"coordinates":[{{longitude}},{{latitude}}]
			}
		}{{#unless @last}},{{/unless}}
		{{/each}}
	]
}