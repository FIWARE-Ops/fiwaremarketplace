window.eventData = [
    {{#each events}}
    {
        "id": "{{yearMonth}}-{{createClass title}}",
        "title": "{{title}}",
        "from": "{{formatISO startTime}}",
        "to": "{{formatISO endTime}}",
        "description": "{{excerpt}}",
        "location": "{{#if (eq venueName '')}}Online{{else}}{{venueName}} {{venueAddress}} {{/if}}",
        "isAllDay": {{#if (eq start '')}}true{{else}}false{{/if}},
        "group": "{{type}}"
    }{{#unless @last}},{{/unless}}
    {{/each}}
];

var modalData = {
{{#each people}}
    {{#if img}}
    "{{createClass name}}":
        { name: "{{title}}{{name}}", img: "{{img}}", position: "{{job}}", company: "{{company}}{{companyType}}", "company-link": "{{website}}",
        content: "{{bio}}", linkedin: "{{linkedIn}}", twitter: "{{twitter}}", domain: "{{domain}}", location: "{{country}}",
        flag: "{{flag}}"}{{#unless @last}},{{/unless}}
    {{/if}}
{{/each}}
};