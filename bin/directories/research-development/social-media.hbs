<html lang="en-US" prefix="og: https://ogp.me/ns#">
<head>
<title>FIWARE R &amp; D Projects: {{name}}</title>
<!-- W3C HTML Tags -->
<meta name="description" content="{{excerpt}}">
<meta name="keywords" content="{{name}},{{domains}},{{#if (gt technologies.length 0)}},{{technologies}}{{/if}}">
<meta http-equiv="refresh" content="0; url=https://www.https://www.fiware.org/about-us/rd-projects/project?id={{createClass name}}" />

<!-- Open Graph Tags -->
<meta property="og:locale" content="en_US">
<meta property="og:type" content="article">
<meta property="og:title" content="FIWARE R &amp; D Projects: {{name}}">
<meta property="og:description" content="{{excerpt}}">
<meta property="og:url" content="https://www.https://www.fiware.org/about-us/rd-projects/project?id={{createClass name}}">
<meta property="og:site_name" content="FIWARE">
<meta property="og:image" content="{{img}}">
<meta property="og:image:secure_url" content="{{img}}">
<meta property="og:image:width" content="1200">
<meta property="og:image:height" content="627">
<meta property="og:image:alt" content="job">
<meta property="og:image:type" content="image/png">

<!-- Twitter Tags -->
<meta name="twitter:card" content="summary_large_image">
<meta name="twitter:title" content="FIWARE R &amp; D Projects: {{name}}">
<meta name="twitter:description" content="{{excerpt}}">
<meta name="twitter:site" content="@FIWARE">
<meta name="twitter:creator" content="@FIWARE">
<meta name="twitter:image" content="{{img}}">
<meta name="twitter:label1" content="Time to read">
<meta name="twitter:data1" content="5 minutes">

<!-- Linked Data -->
<script type="application/ld+json">
{
  "@context": "https://schema.org",
  "@type": "ResearchProject",
  "name": "{{name}}",
  "contactPoint": "{{contact}}",
  "funder": "{{country}}",
  "logo": "{{img}}",
  "description": "{{excerpt}}",
  "url": "https://www.https://www.fiware.org/about-us/rd-projects/project?id={{createClass name}}"
}
</script>
</head>
<body/>
</html>