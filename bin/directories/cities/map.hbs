{
	"type":"FeatureCollection","name":"cities",
	"crs":{"type":"name","properties":{"name":"urn:ogc:def:crs:OGC:1.3:CRS84"}},
	"features":[
	{{#each this}}
		{
			"type":"Feature",
			"properties":{
				"type": "city",
				"html": [
					"<div><img class='gridElementLogo' src='{{img}}'/></div>",
					"<div class='label-location' style='display: flex;flex-direction: row;'><h3>{{city}}</h3>{{#if country}}<img alt='{{country}}' class='flag' src='{{flag}}' style='margin-left: auto;'/>{{/if}}</div><h6>{{country}}</h6>"{{#if title1}},
					"<div class='solution-title'>{{title1}}</div>",
					"<div class='icons'>{{#if website1}}<a class='cat-website' target='_blank' href='{{website1}}' rel='noopener'><span class='icon website'></span></a>{{/if}}{{#if marketplace1}}<a class='cat-website' target='_blank' href='{{marketplace1}}'><span class='icon marketplace'></span></a>{{/if}}{{#if story1}}<a class='cat-website' target='_blank' href='{{story1}}'><span class='icon impactstory'></span></a>{{/if}}</div>"{{/if}}{{#if title2}},
					"<div class='solution-title'>{{title2}}</div>",
					"<div class='icons'>{{#if website2}}<a class='cat-website' target='_blank' href='{{website2}}' rel='noopener'><span class='icon website'></span></a>{{/if}}{{#if marketplace2}}<a class='cat-website' target='_blank' href='{{marketplace2}}'><span class='icon marketplace'></span></a>{{/if}}{{#if story2}}<a class='cat-website' target='_blank' href='{{story2}}'><span class='icon impactstory'></span></a>{{/if}}</div>"{{/if}}
				]
			},
			"geometry":{
				"type":"Point",
				"coordinates":[{{lng}},{{lat}}]
			}
		}{{#unless @last}},{{/unless}}
		{{/each}}
	]
}