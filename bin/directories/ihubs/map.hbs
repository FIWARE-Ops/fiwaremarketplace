var iHub_data = {
	"type":"FeatureCollection","name":"ihubs_4",
	"crs":{"type":"name","properties":{"name":"urn:ogc:def:crs:OGC:1.3:CRS84"}},
	"features":[
	{{#each this}}
		{
			"type":"Feature",
			"properties":{
				"html": [
					"<div><img class='gridElementLogo' src='{{img}}'></div>",
					"<div class='ihub-location'>{{city}}<br>{{country}} {{#if country}}<img alt='{{country}}' class='flag' src='{{flag}}'/>{{/if}}</div>",
					"<div class='ihub-name' colspan='6'>{{name}}</div>",
					"<div class='visible-with-data attribute' id='Type'>Incubated</div>",
					{{#if (gt domain.length 0)}}
					"<div class='label'>Domain</div>",
					"<div>{{#each domain}}{{this}}&nbsp;{{/each}}</div>",
					{{/if}}
					{{#if contacts}}
					"<div class='icons'>{{#if website}}<a class='cat-website' target='_blank' href='{{website}}' rel='noopener'><span class='icon website'></span>{{/if}}{{#if twitter}}<a class='cat-twitter' target='_blank' href='{{twitter}}' rel='noopener'><span class='icon twitter'></span></a>{{/if}}{{#if linkedIn}}<a class='cat-linkedin' target='_blank' href='{{linkedIn}}' rel='noopener'><span class='icon linkedin'></span></a>{{/if}}</div>"
					{{/if}}
				]
			},
			"geometry":{
				"type":"Point",
				"coordinates":[{{longitude}},{{latitude}}]
			}
		},
		{{/each}}
	]
};