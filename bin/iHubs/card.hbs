{{#each this}}
    <div class='grid-item {{createClass type}} {{createClasses domain}} {{createClass country}}'>
        <div class="f-cat active">
            <img decoding="async" class="city-photo" src="{{img}}">
            {{#if country}}
                    <img decoding="async" 
                            alt='{{country}}'
                            class='flag'
                            src='{{flag}}'
                            loading='lazy'
                        />
            {{/if}}
            <h3 class="item-name">{{city}}</h3>
            <h6 class="item-name ihub-name">{{name}}</h6>
            <div class="item-name levels">
                <h2 class="item-name">{{type}}</h2>
            </div>
            {{#if (gt domain.length 0)}}
                {{#each domain}}
                <h3 class="item-name domains-ihub">{{this}}</h3>
                {{/each}}
            {{/if}}
            <div class="icons">
                {{#if website}}
                <a class="cat-website" target="_blank" href="{{website}}" rel="noopener">
                    <span class="icon website"></span></a>
                {{/if}}    
                {{#if twitter}}
                    <a class='cat-twitter' target='_blank' href='{{twitter}}' rel='noopener'><span
                            class='icon twitter'
                        ></span></a>
                {{/if}}
                {{#if linkedIn}}
                    <a class='cat-linkedin' target='_blank' href='{{linkedIn}}' rel='noopener'><span
                            class='icon linkedin'
                        ></span></a>
                {{/if}}
            </div>
        </div>
    </div>
{{/each}}