window.eventData = [
    {{#each events}}
    {
        "id": "{{yearMonth}}-{{createClass title}}",
        "title": "{{title}}",
        "from": "{{formatISO startTime}}",
        "to": "{{formatISO endTime}}",
        "description": "{{excerpt}}",
        "location": "{{#if (eq venueName '')}}Online{{else}}{{venueName}} {{venueAddress}} {{/if}}",
        "isAllDay": {{#if (eq start '')}}true{{else}}false{{/if}},
        "group": "{{type}}"
    }{{#unless @last}},{{/unless}}
    {{/each}}
];