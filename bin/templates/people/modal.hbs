        '{{this}}',
    {{/if}}
{{/each}}
]; var departments = [
{{#each departments}}
    {{#if (ne this '')}}
        '{{this}}',
    {{/if}}
{{/each}}
]; var domains = [
{{#each domains}}
    {{#if (ne this '')}}
        '{{this}}',
    {{/if}}
{{/each}}
]; var titles = [
{{#each filters}}
